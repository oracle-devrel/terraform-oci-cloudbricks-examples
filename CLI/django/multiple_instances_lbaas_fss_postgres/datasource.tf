# datasource.tf
#
# Purpose: The following performs local datasource lookup for home region

data "oci_identity_region_subscriptions" "home_region_subscriptions" {
  tenancy_id = var.tenancy_ocid

  filter {
    name   = "is_home_region"
    values = [true]
  }
}

locals {
  release = "1.0"
}